{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "ui": ["./src/components/ui/index.ts"],
      "@server/*": ["./src/server/*"],
      "@/*": ["./src/*"]
    }
  }
}
